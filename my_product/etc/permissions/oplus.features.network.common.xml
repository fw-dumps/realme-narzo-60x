<!--Tips: The product configuration will not overlay the common configration. Do not configure the same configuration -->

<oplus-config>
    <!-- GuiXiang@NETWORK.REG.8515, 2019/01/03, Add for 5G support feature -->
    <oplus-feature name="oplus.software.radio.support_5g"/>
    <!-- HeZhihui@NETWORK.REG.8515, 2020/06/12, modify for Feature Toggle -->
    <oplus-feature name="oplus.software.radio.nr_icon_optimized_enabled"/>
    <oplus-feature name="oplus.software.radio.ntp_preferred"/>
    <!-- Chenshenghua@NETWORK.Tech, 2021/03/11, add for use vendor ap/modem log pb for logkit-->
    <oplus-feature name="oplus.software.radio.apmdm_log_pb"/>
    <!-- WangHui@NETWORK.REG.2415820, 2021/11/02, add for use sts interface for engnetwork -->
    <oplus-feature name="oplus.software.radio.engnw_sts_interface_support" />
    <!--Cynric.Zhao@NETWORK.DATA add for online meeting-->
    <oplus-feature name="oplus.software.radio.scene_mode" />
    <oplus-feature name="oplus.software.radio.online_meeting" />
    <!-- WangHui@NETWORK.CALL.1981831, 2021/08/13, add for sts support auto answer logic -->
    <oplus-feature name="oplus.software.radio.sts_auto_answer_support" />
    <!--ZhengXueqian@NETWORK.SIM, 2022/03/29, add for using legacy simlock engineer-->
    <oplus-feature name="oplus.software.radio.using_legacy_simlock"/>
    <!-- ZhunNian@NETWORK.DATA.783805, 2015/03/31, modify for not allow internet if we disable internet even when we send mms -->
    <oplus-feature name="oplus.software.radio.mms_not_allow_internet"/>
    <!-- ZengJunyong@NETWORK.DATA, 2022/08/18 for ocloud -->
    <oplus-feature name="oplus.software.logkit.ocloud.support"/>
    <!-- WangHui@NETOWRK.REG,5177547 2023/02/11, Add for sts mipc support -->
    <!-- <oplus-feature name="oplus.software.radio.sts_mipc_support" /> -->
    <!-- Enco.Yuan@NETWORK.REG.2920644, 2022/01/04, add for disable get sim number from IMS Uri-->
    <oplus-feature name="oplus.software.radio.only_get_sim_msisdn"/>
    <!-- Tengju.Cui@NETWORK.IMS.5339983 2023/03/16,Change the value to baseline-->
    <oplus-feature name="oplus.software.radio.sa_ims_control_disabled"/>
</oplus-config>
